{
    "version": 2,
    "buildCommand": "cd mobile && npm install && npm run build",
    "outputDirectory": "mobile/dist",
    "installCommand": "cd mobile && npm install",
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/mobile/dist/$1"
      }
    ]
  }